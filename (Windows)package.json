    "client": "file:client",
    "cloudinary": "^1.23.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "gravatar": "^1.8.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.1.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.9.9",
